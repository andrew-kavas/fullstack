{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "#src/*": ["src/*"]
    },
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowImportingTsExtensions": true
  },
  "include": ["src/**/*"],
}
